	<div class="transport-tabs__content">
    <div class="transport-tabs__panel transport-tabs__panel--active" data-tab-panel="filter">
			<form class="filter-form">
				<div class="filter-form__section filter-form__section--left">
					<div class="filter-form__heading">
						<div class="filter-form__title-wrapper">
							<img src="/icons/truck.svg" alt="Поиск запчастей по авто" width="22">
							<h2 class="filter-form__title">Поиск запчастей по авто</h2>
						</div>
						<p class="filter-form__subtitle">Выберите марку и модель техники, что б проще находить запчасти</p>
					</div>
					<div class="filter-form__fields">
							{% set brand = {
								'name': 'car_brand',
								'options': [
									{'value': 'mercedes', 'label': 'Mercedes'},
									{'value': 'bmw', 'label': 'BMW'},
									{'value': 'audi', 'label': 'Audi'}
								]
							} %}
							{{ include('UI/select.twig', brand) }}

							{% set model = {
								'name': 'car_model',
								'options': [
									{'value': 'mercedes', 'label': 'Mercedes'},
									{'value': 'bmw', 'label': 'BMW'},
									{'value': 'audi', 'label': 'Audi'}
								]
							} %}
							{{ include('UI/select.twig', model) }}

							{% set liter = {
								'name': 'car_liter',
								'options': [
									{'value': 'mercedes', 'label': 'Mercedes'},
									{'value': 'bmw', 'label': 'BMW'},
									{'value': 'audi', 'label': 'Audi'}
								]
							} %}
							{{ include('UI/select.twig', liter) }}
							<button class="btn btn-primary btn-md">Применить</button>
					</div>
				</div>
				<div class="filter-form__section filter-form__section--right">
					<h2 class="filter-form__title">Популярные марки</h2>
					<div class="filter-form__suggestions">
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
						<button class="filter-form__suggestion btn btn-outline">КамАЗ</button>
					</div>
				</div>
			</form>
		</div>
    <div class="transport-tabs__panel" data-tab-panel="garage">
			<div class="garage">
				{% set one = {
					'isActive': true,
					'name': 'Mersedes Actross 2016',
					'config': '10l, diesel, automatic'
				} %}
				{{ include("components/garage-car.twig", one) }}

				{% set two = {
